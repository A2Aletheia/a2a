{
  "name": "@a2aletheia/a2a",
  "version": "0.2.9",
  "description": "Aletheia trust-layer wrapper for the A2A (Agent-to-Agent) protocol",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/A2Aletheia/a2a",
  "bugs": "https://github.com/A2Aletheia/a2a/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "a2a",
    "agent",
    "trust",
    "aletheia",
    "multi-agent",
    "ai-agents",
    "agent-to-agent"
